{
  "name": "pathogen",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cis3296f22.github.io/Pathogen",
  "author": "Ryan Hodge, Ben Scuron, Nick Coffin, Kai Iwugo",
  "contributors": [
    "Ryan Hodge",
    "Ben Scuron",
    "Nick Coffin",
    "Kai Iwugo"
  ],
  "description": "A genetic path-finding algorithm visualization",
  "main": "./public/electron.js",
  "build": {
    "appId": "com.electron.pathogen",
    "productName": "Pathogen",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.68",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "canvas": "^2.10.2",
    "rc-tabs": "^12.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-slider": "^2.0.4",
    "typedoc": "^0.23.21",
    "react-tabs": "^5.1.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "electron:build": "PUBLIC_URL=./ react-scripts build",
    "electron:start": "PUBLIC_URL=./ concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "PUBLIC_URL=./ yarn electron:build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "PUBLIC_URL=./ yarn electron:build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "PUBLIC_URL=./ yarn electron:build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.186",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react-slider": "^1.3.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^29.3.1",
    "bootstrap": "^5.2.2",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "gh-pages": "^4.0.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-device-detect": "^2.2.2",
    "react-icons": "^4.6.0",
    "react-input-slider": "^6.0.1",
    "react-p5": "^1.3.30",
    "react-range-slider-input": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.56.1",
    "styled-components": "^5.3.6",
    "typedoc": "^0.23.21",
    "uuidv4": "^6.2.13",
    "wait-on": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cis3296f22/Pathogen.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cis3296f22/Pathogen/issues"
  }
}
